@page
@model WebThreetier.Pages.Email.ValidateModel

@if (ViewData["valid"] != null)
{
    if ((bool)ViewData["valid"])
    {
        <span>激活成功</span>
    }
    else
    {
        <span>激活失败</span>
    }

}
else
{
    <span>异常状态</span>
}

<form method="post">
    <label>Email:</label><input asp-for="EmailAddress" />
    <button type="submit">提交</button>
</form>

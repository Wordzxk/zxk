    Layout = null;
}
<html>
<head>
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />

    <link href="~/css/bootstrap.min.css" rel="stylesheet" media="screen">

    <style>
        div.box-1 {
            box-sizing: padding-box;
            padding: 1em;
            width: 100%;
            background-color: #e7e7e7;
        }

        /*body{
            text-align:center;
